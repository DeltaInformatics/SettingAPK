<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/color_normal_button_background_1" />
            <gradient android:startColor="@color/color_tailgate_button_start_color_1" android:endColor="@color/color_tailgate_button_end_color_1" android:angle="270.0" />
            <corners android:radius="8.0px" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <gradient android:startColor="@color/color_normal_button_pressed_start_color_background_1" android:endColor="@color/color_normal_button_pressed_end_color_background_1" android:angle="270.0" />
            <corners android:radius="8.0px" />
        </shape>
    </item>
</selector>
