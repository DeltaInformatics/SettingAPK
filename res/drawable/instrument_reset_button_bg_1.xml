<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/color_normal_button_background_1" />
            <gradient android:startColor="@color/color_meter_frd_bg_not_picked_start_1" android:endColor="@color/color_meter_frd_bg_not_picked_end_1" android:angle="270.0" />
            <corners android:radius="12.0px" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <gradient android:startColor="@color/color_meter_frd_bg_is_picked_start_1" android:endColor="@color/color_meter_frd_bg_is_picked_end_1" android:angle="270.0" />
            <corners android:radius="12.0px" />
        </shape>
    </item>
</selector>
