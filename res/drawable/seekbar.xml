<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <size android:height="80.0px" />
            <corners android:radius="8.0px" />
            <solid android:color="@color/seekbar_progress_bgcolor" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip android:scaleWidth="100%">
            <shape>
                <size android:height="80.0px" />
                <corners android:radius="8.0px" />
                <gradient android:startColor="@color/seekbar_progress_bg_start_color" android:endColor="@color/seekbar_progress_bg_end_color" android:angle="0.0" />
            </shape>
        </clip>
    </item>
</layer-list>
