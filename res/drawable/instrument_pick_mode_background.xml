<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape android:shape="rectangle">
            <corners android:radius="12.0px" />
            <stroke android:width="2.0px" android:color="@color/color_meter_mode_img_bg_select" />
        </shape>
    </item>
    <item android:state_selected="false">
        <shape android:shape="rectangle">
            <corners android:radius="12.0px" />
            <stroke android:width="2.0px" android:color="@color/color_meter_mode_img_bg_unselect" />
        </shape>
    </item>
</selector>
